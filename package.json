{
  "name": "jowna",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "deploy:docs": "npm run build && mkdir -p docs && rsync -a --delete dist/ docs/ && touch docs/.nojekyll",
    "publish:site": "python3 tools/publish.py",
    "preview": "vite preview",
    "typecheck": "tsc -b --pretty false",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"*.{json,md,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"*.{json,md,ts,tsx}\""
  },
  "dependencies": {
    "@owebeeone/grip-react": "^0.2.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
